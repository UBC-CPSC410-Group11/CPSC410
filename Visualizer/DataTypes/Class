class Class:
	name = ""
	package = ""
	module = ""
	methods = []
	outCalls = []
	
	def __init__(self, in_Name, in_Package, in_Module)
		self.name = in_Name
		self.package = in_Package
		self.module = in_Module
		
	addMethod(in_Method)
		methods.append(in_Method)
		
	addOutCall(in_ClassName)
		outCalls.append(in_ClassName)
	
	def getOutCalls()
		return outCalls
		
	def getMethods()
		return methods
	
